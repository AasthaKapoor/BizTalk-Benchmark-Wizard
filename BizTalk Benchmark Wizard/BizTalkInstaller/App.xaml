<Application x:Class="BizTalkInstaller.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    StartupUri="Window1.xaml">
    <Application.Resources>
         <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles\Shared.xaml"/>
                <ResourceDictionary Source="Styles\ScrollBarColors.xaml"/>
                <ResourceDictionary Source="Styles\ScrollBar.xaml"/>
                <ResourceDictionary Source="Styles\Expander.xaml"/>
                <ResourceDictionary Source="Styles\GroupBox.xaml"/>
                <ResourceDictionary Source="Styles\ToolTip.xaml"/>
                <ResourceDictionary Source="Styles\ScrollViewer.xaml"/>
                <ResourceDictionary Source="Styles\Checkbox.xaml"/>
