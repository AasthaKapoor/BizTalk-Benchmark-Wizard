<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <connectionStrings>
    <add name="BizTalkMgmtDatabase" connectionString="Data Source={0};Initial Catalog={1};Integrated Security=SSPI;" />
  </connectionStrings>
  <appSettings>
    <add key="GetSqlServersQuery" value="SELECT [BizTalkAdminGroup]
                                ,[TrackingDBServerName]
                                ,[SubscriptionDBServerName]
                                ,[BamDBServerName]
                                ,[RuleEngineDBServerName]
                            FROM [BizTalkMgmtDb].[dbo].[adm_Group]"/>
    
    <add key="GetMsgBoxServersQuery" value="SELECT [DBServerName]
                                FROM [dbo].[adm_MessageBox]"/>

    <add key="GetBizTalkServersQuery" value="SELECT [Name]
                                FROM [BizTalkMgmtDb].[dbo].[adm_Server]"/>

    <add key="GetComputerNameQuery" value="select CONVERT(SYSNAME, SERVERPROPERTY('ComputerNamePhysicalNetBIOS')) as COMPUTERNAME"/>
  </appSettings>

  <system.net>
    <defaultProxy>
      <proxy autoDetect="false"/>
    </defaultProxy>

    <connectionManagement>
      <add address = "*" maxconnection = "400" />
    </connectionManagement>
  </system.net>

  <system.diagnostics>
    <trace autoflush="true" indentsize="4">
      <listeners>
        <add name="LogConsoleListener" type="System.Diagnostics.ConsoleTraceListener"/>
        <add name="myListener" type="System.Diagnostics.TextWriterTraceListener" initializeData="logs\LoadGenEAF.log" />
      </listeners>
    </trace>
  </system.diagnostics>

  <system.serviceModel>

    <client>
      <endpoint name="ClientEndPoint1"
                address="net.tcp://IDCVDEV02:1001/TCP1"
                binding="netTcpBinding"
                bindingConfiguration="Binding1"
                contract="WcfTransport.IChannelAsyncNonTransactional"/>
      <endpoint name="ClientEndPoint2"
                   address="net.tcp://IDCVDEV02:1002/TCP1"
                   binding="netTcpBinding"
                   bindingConfiguration="Binding1"
                   contract="WcfTransport.IChannelAsyncNonTransactional"/>
      <endpoint name="ClientEndPoint3"
                   address="http://IDCVDEV02/Xlang_EmptySchedule/EmptySchedule_Baseline1_SimpleSchedule_Port_1.svc"
                   binding="wsHttpBinding"
                   bindingConfiguration="Bindingws"
                   contract="WcfTransport.IChannelAsyncNonTransactional"/>
      <endpoint name="ClientEndPoint4"
                      address="net.msmq://IDCVDEV02/private/q1"
                      binding="netMsmqBinding"
                      bindingConfiguration="Bindingnetmsmq"
                      contract="WcfTransport.IChannelOneWay"/>
      <endpoint name="ClientEndPoint5"
                      address="net.msmq://tap2x-l07/q1"
                      binding="netMsmqBinding"
                      bindingConfiguration="Bindingnetmsmq"
                      contract="WcfTransport.IChannelOneWay"/>
    </client>

    <bindings>
      <netTcpBinding>
        <binding name="Binding1" transactionFlow="false" transactionProtocol="OleTransactions"
                 openTimeout="23:59:59" receiveTimeout="23:59:59" closeTimeout="23:59:59" sendTimeout="23:59:59" maxConnections="1000">
          <security mode="None"></security>
        </binding>
      </netTcpBinding>
      <wsHttpBinding>
        <binding name="Bindingws" openTimeout="23:59:59" receiveTimeout="23:59:59" closeTimeout="23:59:59" sendTimeout="23:59:59">
          <security mode="None">
          </security>
        </binding>
      </wsHttpBinding>
      <netMsmqBinding>
        <binding name="Bindingnetmsmq" durable="false" exactlyOnce="false" openTimeout="23:59:59" receiveTimeout="23:59:59" closeTimeout="23:59:59"  sendTimeout="23:59:59">
          <security mode="None">
          </security>
        </binding>
      </netMsmqBinding>
    </bindings>

  </system.serviceModel>
</configuration>